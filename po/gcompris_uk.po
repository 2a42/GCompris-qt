msgid "translator-credits"
msgstr "Онищук Іван"

#: core/DialogAbout.qml:42
#, qt-format
msgctxt "DialogAbout|"
msgid "GCompris %1"
msgstr "GCompris %1"

#: core/DialogAbout.qml:43
#, qt-format
msgctxt "DialogAbout|"
msgid "Based on Qt %1"
msgstr "На основі Qt %1"

#: core/DialogAbout.qml:47
